@model StudentskiHotel.Web.Areas.ModulAdministrator.ViewModels.DodajSpratVM
@{
    string poruka = (string)ViewData["poruka"];
}
<form action="/ModulAdministrator/Sprat/SnimiSprat" autocomplete="off" ajax-poziv="da" ajax-rezultat="ajaxdiv">
    <table>
        <tr>
            <td>@Html.HiddenFor(x => x.Id)</td>
        </tr>
        <tr>
            <td>Sprat:</td>
            <td>@Html.TextBoxFor(x => x.BrojSprata, new { @class = "form-control" })</td>
            <td>@Html.ValidationMessageFor(x => x.BrojSprata)</td>
            <td>@poruka</td>
        </tr>

        <tr><td><input type="submit" value="Snimi" class="btn btn-dark" /></td></tr>

    </table>
</form>

